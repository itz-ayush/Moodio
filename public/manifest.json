{
  "manifest_version": 3,
  "name": "MOODIO",
  "version": "1.0",
  "description": "Productivity booster with work modes and focus timer",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "alarms",
    "notifications"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "MOODIO"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "moodio.png",
    "48": "moodio.png",
    "128": "mooodio.png"
  }
}